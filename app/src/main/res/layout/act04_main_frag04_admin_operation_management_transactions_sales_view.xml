<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/M04F04OPTSV_Layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/dirtywhite">

    <ImageView
        android:id="@+id/M04F04OPTSV_Receipt"
        android:layout_width="150dp"
        android:layout_height="150dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.04"
        app:srcCompat="@drawable/icon_transaction" />

    <TextView
        android:id="@+id/M04F04OPTSV_TID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nunito_sans_light"
        android:text="TID"
        android:textColor="@color/wabizabi"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/M04F04OPTSV_Receipt"
        app:layout_constraintVertical_bias="0.019" />

    <TextView
        android:id="@+id/M04F04OPTSV_Items"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nunito_sans_light"
        android:text="Items"
        android:textColor="@color/wabizabi"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/M04F04OPTSV_TID"
        app:layout_constraintVertical_bias="0.025" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/M04F04OPTSV_RecyclerView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toTopOf="@+id/M04F04OPTSV_SubTotalTxt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/M04F04OPTSV_Items"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/M04F04OPTSV_SubTotalTxt"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nunito_sans_light"
        android:padding="10dp"
        android:text="S u b - T o t a l"
        android:textColor="@color/wabizabi"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/M04F04OPTSV_RefundButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.144"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/M04F04OPTSV_Receipt"
        app:layout_constraintVertical_bias="0.771" />

    <TextView
        android:id="@+id/M04F04OPTSV_SubTotalNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nunito_sans_light"
        android:padding="10dp"
        android:text="₱ 0.00"
        android:textColor="@color/wabizabi"
        android:textSize="25sp"

        app:layout_constraintBottom_toTopOf="@+id/M04F04OPTSV_RefundButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/M04F04OPTSV_SubTotalTxt"
        app:layout_constraintTop_toBottomOf="@+id/M04F04OPTSV_Receipt"
        app:layout_constraintVertical_bias="0.771" />

    <TextView
        android:id="@+id/M04F04OPTSV_TaxTxt"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="132dp"
        android:fontFamily="@font/nunito_sans_light"
        android:padding="10dp"
        android:text="T a x  ( 3% )"
        android:textColor="@color/wabizabi"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.145"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/M04F04OPTSV_SubTotalTxt"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/M04F04OPTSV_TaxNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nunito_sans_light"
        android:padding="10dp"
        android:text="₱ 0.00"
        android:textColor="@color/wabizabi"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/M04F04OPTSV_TaxTxt"
        app:layout_constraintTop_toBottomOf="@+id/M04F04OPTSV_SubTotalNo"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/M04F04OPTSV_TotalTxt"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:fontFamily="@font/nunito_sans_light"
        android:padding="10dp"
        android:text="T o t a l"
        android:textColor="@color/wabizabi"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/M04F04OPTSV_RefundButton"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.145"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/M04F04OPTSV_TaxTxt"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/M04F04OPTSV_TotalNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="10dp"
        android:fontFamily="@font/nunito_sans_light"
        android:padding="10dp"
        android:text="₱ 0.00"
        android:textColor="@color/wabizabi"

        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/M04F04OPTSV_RefundButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/M04F04OPTSV_TotalTxt"
        app:layout_constraintTop_toBottomOf="@+id/M04F04OPTSV_TaxNo"
        app:layout_constraintVertical_bias="0.0" />

    <androidx.cardview.widget.CardView
        android:id="@+id/M04F04OPTSV_RefundButton"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:layout_margin="15dp"
        android:backgroundTint="@color/wabizabi"
        android:elevation="100dp"
        app:cardCornerRadius="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.98">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/nunito_sans_light"
                android:text="Refund"
                android:textColor="@color/white"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>